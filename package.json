{
  "name": "chunithm-location-map",
  "version": "1.0.0",
  "description": "チュウニズム設置場所マップアプリ",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ChunithmLocationMap.xcworkspace -scheme ChunithmLocationMap -configuration Release -archivePath build/ChunithmLocationMap.xcarchive archive",
    "web": "webpack serve --config webpack.config.js --mode development",
    "build:web": "webpack --config webpack.config.js --mode production"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.73.0",
    "react-native-web": "^0.19.9",
    "react-native-maps": "^1.15.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-permissions": "^4.1.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-gesture-handler": "^2.14.0",
    "@reduxjs/toolkit": "^2.0.0",
    "react-redux": "^9.0.0",
    "redux-persist": "^6.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-paper": "^5.12.0",
    "react-native-svg": "^14.1.0",
    "react-native-reanimated": "^3.6.0",
    "@react-native-community/netinfo": "^11.2.0",
    "@react-native-community/slider": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/metro-config": "^0.73.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/jest-native": "^5.4.3",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^2.4.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "babel-loader": "^9.1.3",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@types/jest": "^29.2.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "@types/react-native-vector-icons": "^6.4.18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}